{
  "name": "vite-react-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint_fix": "eslint . --fix",
    "prepare": "cd ../.. && husky full_app/frontend/.husky",
    "prettier": "prettier . --write --ignore-unknown",
    "test": "vitest --root tests/",
    "test:coverage": "vitest --coverage",
    "build:css": "tailwindcss -i src/styles/index.css -o dist/styles.css"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run prettier --prefix full_app/frontend",
      "npm run lint --prefix full_app/frontend"
    ]
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  },
  "devDependencies": {
    "@testing-library/react": "^15.0.7",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "3.2.5",
    "tailwindcss": "^3.4.3",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^1.6.0"
  }
}
